          <div id="box_form" class="form" ng-show="form">
            <form name="boxForm">
              <div class="table">
                <div class="row name">
                  <div class="cell label">Name:</div>
                  <div class="cell value"><input type="text" name="name" ng-change="check_name_options()" required ng-model="src.name"></div>
                </div>
                <div class="row volume">
                  <div class="cell label">Volume:</div>
                  <div class="cell value"><input type="number" min="0" required ng-model="src.volume"></div>
                </div>
              </div>
            </form>
          </div>
